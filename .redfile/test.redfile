---
qed:
  service: Custom
  track  : main
  test   : |
    result = system "qed -Ilib qed/"
    exit -1 unless result

syntax:
  service  : Syntax
  loadpath : ~
  exclude  : ~
  active   : false

testrb:
  service  : testrb
  tests    : ~
  exclude  : ~
  loadpath : ~
  requires : ~
  live     : false   
  active   : false

